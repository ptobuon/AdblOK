{
    "name": "AdblOK",
    "version": "1.0",
    "description": "A simple Adblocker",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action":{
        "default_popup": "popup.html"
    },
    "icons": {
        "48": "icons/icon.png"
    },
    "manifest_version": 2
}